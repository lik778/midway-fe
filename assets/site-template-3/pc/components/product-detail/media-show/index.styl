@import 'swiper/dist/css/swiper.min.css'
@import "../../../../../common/pc/official-400-btn";
.my-slide-active
  border: 1px solid #ED4646
.media-show-container
  display: flex
  justify-content space-between
  padding: 10px 0 0px 0
  margin-bottom 20px
  background-color: #FFFFFF
  .media-show-left
    width: 406px
    .left-headImg
      margin-bottom: 20px
      border-radius: 4px
      overflow hidden
      .video-con
        position relative
        height 270px
        width 406px
        flex-shrink 0
        .video-wrapper
          display flex
          justify-content center
          align-items center
          width 100%
          height 100%
          background-color black
          video
            flex 1
            width 100%
            height 100%
            cursor pointer
            z-index 9
        .video-cover
          position absolute
          top 0
          left 0
          width 100%
          height 100%
          z-index 9
          cursor pointer
          img
            width 100%
            height 100%
        .video-cover::after
          content ''
          position absolute
          left calc(50% - 28px)
          top calc(50% - 28px)
          width 55px
          height 55px
          background url("data:image/png;base64,")
          background-size 100%
          cursor pointer
        .video-img
          position absolute
          top 0
          left 0
          width 100%
          height 100%
          z-index 9
          // cursor pointer
          img
            width 100%
            height 100%
        .video-cover::after
          content: ''
      img
        height 270px
        width 406px
        border-radius 4px
    .left-contentImg
      width: 100%
      height: 72px
      .swiper-container
        width: 100%
        height: 100%
        .swiper-button-prev
          left: 5px
          width: 30px
          height: 30px
          background: url('http://file.baixing.net/202105/05348a4efc053232fdd668940bd756a8.png') no-repeat center center/100%
          &:hover
            background url(//file.baixing.net/202108/a74c1d873881392c12a23826cd4e1a37) no-repeat center center/100%
            transform rotate(0)
        .swiper-button-next
          right: 5px
          width: 30px
          height: 30px
          background: url('http://file.baixing.net/202105/05348a4efc053232fdd668940bd756a8.png') no-repeat center center/100%
          transform: rotate(180deg)
          &:hover
            background url(//file.baixing.net/202108/af2916a86a85a940625a4a47c70d5fb3) no-repeat center center/100%
            transform rotate(0)
        .swiper-slide
          position relative
          width: 99px
          height: 66px
          margin-right: 7px
          border-radius: 4px
          display: -webkit-box;
          display: -ms-flexbox;
          display: -webkit-flex;
          display: flex;
          -webkit-box-pack: center;
          -ms-flex-pack: center;
          -webkit-justify-content: center;
          justify-content: center;
          -webkit-box-align: center;
          -ms-flex-align: center;
          -webkit-align-items: center;
          align-items: center;
          img 
            width: 100%
            height: 100%
            border-radius: 4px
          .video-cover
            position absolute
            top: 0
            left: 0
            width 100%
            height 100%
            z-index 999
            cursor pointer
          .video-cover::after
            content ''
            position absolute
            left calc(50% - 22px)
            top calc(50% - 21px)
            width 42px
            height 44px
            background url("data:image/png;base64,")
            background-size 100%
            cursor pointer
      .gallery-thumbs
        .swiper-slide-thumb-active
          border: 1px solid #ED4646
  .media-show-middle
    width: 100%
    margin 15px 0 0 20px
    .detail-title
      width: 100%
      overflow: hidden
      text-overflow:ellipsis
      white-space: nowrap
      margin-bottom 24px
      color: #333333
      font-size: 18px
      font-weight: bold
    .media-middle-left
      display: flex
      justify-content: space-between
      .product-left
        .detail-price
          box-sizing: border-box
          display: flex
          justify-content: flex-start
          align-items: center
          width: 100%
          padding-left: 9px
          color: #ED4646
          font-size: 14px
          font-weight: bold
          img
            width: 28px
            height: 23px
            margin-right: 7px
          .prices
            margin-left: 24px
            font-size: 20px
            font-weight: 500px
        .detail-setting
          padding: 24px 0px 12px 0px
          margin-bottom:12px
          color: #333333 
          font-size: 12px
          .setting-title
            font-size: 14px
            font-weight: bold
            margin-bottom 8px
            text-align: left
          .detail-product
            .detail-table
              overflow hidden
              table
                border-collapse: collapse
                overflow hidden
                tr
                  display: flex
                  .setting-key
                    display: flex
                    justify-content flex-start
                    align-items center
                    width: 120px
                    padding: 12px 0 12px 16px
                    font-size: 12px
                    color: #999999
                    border-right: 1px solid #EEEEEE
                    border-bottom: 1px solid #EEEEEE
                  .setting-value
                    display: flex
                    justify-content flex-start
                    align-items center
                    width: 388px
                    padding: 12px 0 12px 16px
                    color: #333333
                    font-size: 12px
                    border-bottom: 1px solid #EEEEEE
              table
                border: 1px solid #EEEEEE
                border-bottom: 0
            .read-more
              display: none
              height: 32px
              width: 541px
              line-height 32px
              text-align: center
              color: #999999
              border: 1px solid #EEEEEE
              border-top: 0 !important
              cursor pointer
              z-index 999
        .detail-contact
          display: flex
          justify-content flex-start
          align-items center
          .online-btn
            display: flex
            justify-content: center
            align-items: center
            width: 140px
            height: 40px
            border: 1px solid #ED4646
            font-size: 14px
            color: #ED4646
            border-radius: 4px
            img 
              width: 20px;
              height: 19px
              margin-right 8px
