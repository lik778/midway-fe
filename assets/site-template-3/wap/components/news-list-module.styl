.news-list-title-box
    display flex
    justify-content space-between
    align-items center
    padding 0 20px 0 15px
    height 58px
    background-color #fff
    border-bottom 1px solid #E2E2E2
    .title
        height 18px
        font-size 18px
        font-weight bold
        color #333333
        line-height 18px
    .more
        font-size 14px
        color #AAAAAA
        line-height 14px