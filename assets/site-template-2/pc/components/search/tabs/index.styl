.search-tabs
  margin-bottom 24px
  padding: 0 24px
  background-color: #fff
  .tabs
    display: flex
    justify-content: flex-start
    align-items: flex-start
    line-height: 62px
    border-bottom 1px solid #EEEEEE
    .tab
      font-size: 14px;
      color: #999999;
      line-height: 62px;
      cursor pointer
      &.active
        font-weight: bold;
        color: #333333;
        border-bottom: 2px solid #333
      &+.tab
        margin-left 30px
  .tip
    font-size: 14px;
    color: #333333;
    line-height: 62px
      
