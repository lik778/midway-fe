.topbar-bg
	width 100%
	background-color #333
	.topbar
		width 1220px
		height 44px
		line-height 44px
		margin 0 auto
		position relative
		span
			font-size 14px
			color #fff
		img
			vertical-align middle
			&:first-child
				margin-right 10px
				width 15px
				height 15px
			&:last-child
				width 44px
				height 22px
				position absolute
				right 0
				top 11px


