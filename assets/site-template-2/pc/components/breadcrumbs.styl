.search-crumbs
	width 1220px
	margin 30px auto
	display flex
	align-items center
	p
		width 80px
		font-size 16px
		color #666
	ul
		li
			display inline-block
			margin-right 8px
			.right
				margin-bottom 6px
			a,span
				display inline-block
				font-size 16px
				color #666
				margin-left 10px
				max-width 255px
				overflow hidden
				text-overflow ellipsis
				white-space nowrap





