@import "../../../../common/wap/contact-phone-btn"
.contact-box
  position fixed
  bottom 0
  width 100%
  background #fff
  display flex
  align-items center
  padding 10px 15px
  box-sizing border-box
  justify-content space-between
  .bx-logo
    width 45px
    height 45px
    background url('//file.baixing.net/202102/f4cc4e3a39f50595a2560b5376152ee0.png') no-repeat center center/100%
    margin 0 25px 0 10px

  .online-box
    flex 1
    margin-right 15px
    a
      display block
      height 40px
      line-height 40px
      text-align center
      font-size 14px
      border-radius 2px
      border 1px solid transparent
      box-sizing border-box
      background-color #fff
      color #EC2525
      border-color #EC2525