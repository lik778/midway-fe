.tab-header
    width 100%
    background-color #ffffff
    font-size 16px
    font-weight 600
    ul
        display flex
        align-items center
        justify-content space-between
        margin 0 12px
        height 48px
        border-top 1px solid #f2f2f2
        margin-bottom 22px
        li
            display inline-block
            list-style-type none
            margin-right 26px
            a
                font-size 12px
                text-decoration none
                color #666
                &:focus
                    color #1565C1
        img
            display inline-block
            width 16px
            height 16px
    .all
        display none

    .tab-active
        color #1565C1 !important



