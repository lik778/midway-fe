.contact-tel
  display: flex
  flex-direction: column
  justify-content: center
  align-items: center
  width: 100%
  height: 140px
  .tel-title
    display: flex
    justify-content: center
    align-items: center
    .title-icon
      width: 38px
      height: 38px
      img
        width: 100%
        height: 100%
    .titel-hot 
      width: 255px
      height: 32px
      margin: 0 9px 0 14px
      img
        width: 100%
        height: 100%
    .titel-number
      font-size: 36px;
      color: #FFFFFF;
      font-weight: bold;
      letter-spacing: 1px;
    .titel-consult
      display: inline-block
      width: 160px;
      height: 40px;
      line-height 40px
      text-align: center
      margin-left: 20px
      background: #FDAB00;
      border-radius: 20px;
      font-weight: bold
      cursor pointer
  .tel-content
    margin-top: 19px
    text-align: center
    color: #FFFFFF;
    font-size: 16px;
    letter-spacing: 1.0