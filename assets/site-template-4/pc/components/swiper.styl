.banner-content
  height: 630px
  position relative
  margin 0 auto
  display flex
  justify-content center
  align-items center
  overflow hidden
  .bg-blur
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%
    z-index: 0
    filter: blur(10px);
  #banner-container
    width 1200px
    position relative
    height 300px
    .swiper-slide
      width 329.4px
      height 274px
      overflow hidden
      position relative
      border-radius 4px 4px 0 0
      &.video
        &::after
          content ''
          position absolute
          left 50%
          top 32%
          width 55px
          height 55px
          transform: translateX(-50%)
          background url("data:image/png;base64,")
          background-size 100%
          cursor pointer
      img
        display block
        width 100%
        height: 219px;
      p
        box-sizing border-box
        width 100%
        height 54px
        margin 0
        padding 0 40px
        line-height 54px
        text-align center
        color #333333
        font-size 16px
        background-color #fff
        box-shadow 0 4px 10px rgba(#000, 0.2)
        text-shadow: 0px 0px 27px rgba(0, 0, 0, 0.15)
    .swiper-pagination
      width 100%
      bottom 44px
    .swiper-pagination-bullets
      .swiper-pagination-bullet
        margin-right: 18px
        background: #fff
        width 100px
        height 4px
        border-radius: 0
        opacity 1.0
    .swiper-button-prev
      left 4%
      width 60px
      height 60px
      background url(//file.baixing.net/202105/05348a4efc053232fdd668940bd756a8.png) no-repeat center center/100%
      &:hover
        background url(//file.baixing.net/202108/a74c1d873881392c12a23826cd4e1a37) no-repeat center center/100%
        transform rotate(0)
    .swiper-button-next
      right 4%
      width 60px
      height 60px
      top 50%
      background url(//file.baixing.net/202105/05348a4efc053232fdd668940bd756a8.png) no-repeat center center/100%
      transform rotate(180deg)
      &:hover
        background url(//file.baixing.net/202108/af2916a86a85a940625a4a47c70d5fb3) no-repeat center center/100%
        transform rotate(0)
#banner-list
  width: 100%
  .swiper-container
    width: 100%;
    .swiper-wrapper
      width 100%
      .swiper-slide
        width: 100%
        height: 630px
        min-height: 343.125px
        position relative
        img
          width: 100%
          height: 100%
    .swiper-pagination-bullets
      bottom: 44px
      .swiper-pagination-bullet
        width 100px
        height 4px
        margin-right: 18px
        background: #fff
        border-radius: 0
        opacity 1.0
    .swiper-button-prev
      left 4%
      width 60px
      height 60px
      background url(//file.baixing.net/202105/05348a4efc053232fdd668940bd756a8.png) no-repeat center center/100%
      &:hover
        background url(//file.baixing.net/202108/a74c1d873881392c12a23826cd4e1a37) no-repeat center center/100%
        transform rotate(0)
    .swiper-button-next
      right 4%
      width 60px
      height 60px
      top 50%
      background url(//file.baixing.net/202105/05348a4efc053232fdd668940bd756a8.png) no-repeat center center/100%
      transform rotate(180deg)
      &:hover
        background url(//file.baixing.net/202108/af2916a86a85a940625a4a47c70d5fb3) no-repeat center center/100%
        transform rotate(0)

/* 轮播视频 */
#banner-list
  .video-wrapper
    display flex
    justify-content center
    align-items center
    width 100%
    height 100%
    background-color black
    video
      flex 1
      width 100%
      height 100%
      cursor pointer
      z-index 9
  .video-cover
    position absolute
    top 0
    left 0
    width 100%
    height 100%
    z-index 9
    cursor pointer
    img
      width 100%
      height 100%
  .video-cover::after
    content ''
    position absolute
    left calc(50% - 28px)
    top calc(50% - 28px)
    width 55px
    height 55px
    background url("data:image/png;base64,")
    background-size 100%
    cursor pointer
