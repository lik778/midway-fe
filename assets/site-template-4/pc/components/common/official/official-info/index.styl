@import '../official-info-phone/index.styl'
.official-info-container 
  background-color: #fff
  border-bottom: 1px solid #f0f0f0
  .official-info-content
    display: flex
    justify-content: space-between
    align-items: center
    width 1200px
    height: 98px
    margin: 0 auto
    &.sem-2
      height: 50px
    .left
      display: flex
      justify-content: flex-start
      align-items: center
      .logo 
        height 88px
        max-width: 191px
        margin-right 15px
      .line 
        width 6px
        height: 56px
        margin-right 20px
        background: url('//file.baixing.net/202204/2ab2976101a2a512594763fff397d790.png') no-repeat center center/cover
      .text
        display: flex
        flex-direction: column
        justify-content: space-between
        .title
          font-size: 28px
          font-weight: bold
        .subtitle
          margin-top: 7px
          font-size: 16px
          color #646464

