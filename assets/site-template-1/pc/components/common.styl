@charset "utf-8";
/* CSS Document */
body, div
	margin 0
	padding 0
	font-family "Microsoft YaHei"
	color #555555

a
	text-decoration none
	cursor pointer
	&:hover
		color #1565C1 !important

//解决浮动问题
.clearfix:before,
.clearfix:after
	content ""
	display table

.clearfix:after
	clear both

//解决IE6,7不识别伪元素
.clearfix
	zoom 1

img
	border 0

ul,li,dl,dt,dd
	list-style none
	margin 0
	padding 0

h1,h2,h3,h4,h5,h6
	padding 0
	margin 0
