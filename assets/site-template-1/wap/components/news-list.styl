.news-list
    width 100%
    line-height 44px
    ul
        display block
        margin 0px 15px 0px 0px
        li
            list-style none
            border-bottom 1px dashed
            a
                text-decoration none
                p   
                    letter-spacing 0
                    color #333
                    font-size 16px
                    overflow hidden
                    white-space nowrap
                    text-overflow ellipsis




