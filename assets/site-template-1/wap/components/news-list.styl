.news-list
    width 100%
    margin-top 18px
    line-height 60px
    min-height 240px
    background-color #ffffff
    ul
        margin 0 20px 20px
        li
            list-style none
            border-bottom 1px dashed
            a
                text-decoration none
                p
                    letter-spacing 0
                    color #333
                    font-size 16px
                    overflow hidden
                    white-space nowrap
                    text-overflow ellipsis




