@import "../contact-phone-btn"
.official-footer-contact-container
  position fixed
  bottom 0
  width 100%
  background #fff
  display flex
  align-items center
  padding 10px 7px
  box-sizing border-box
  justify-content space-between
  .wechat-box,.online-box
    display: flex
    flex-direction: column
    justify-content: center
    align-items: center
    padding: 0 20px
    .text
      font-size: 14px;
      color: #333333;
  .wechat-box
    .icon
      width: 23px
      height: 21px
  .online-box
    .icon
      width: 21px
      height: 22px

