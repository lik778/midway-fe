.page-normal
	text-align center
	margin-top 45px
	.page-item
		display: inline
		color #333
		cursor pointer
	.page-item,
	.page-current
		border 1px solid#E5E5E5
		padding 5px 11px
		text-decoration none
		margin-left 6px

//未选中的分页页码样式，和鼠标滑过样式如下
.page-normal
	.page-current,
	.page-item:hover
		color #FFF !important
		background-color #FF2D38
