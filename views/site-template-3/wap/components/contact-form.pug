//- #contactForm 用于sem跳转
.contact-form#contactForm
	h4.block-title 留言咨询
	- const {contactFields}  = renderData.basic.shop
	- const list = contactFields.filter(item => item.position === 'content')
	- const btn = contactFields.filter(item => item.title === '表单按钮')[0].des
	.contact-form-list
		each item in list.length  && list
			.a-content-item
				.item-label
					| #{item.title}
					span.labe-icon &nbsp;*
				input(type="text" name="contact" data-value=item.des data-key=item.title placeholder=item.des)
		.form-item
			button#upContactForm.btn-animate-light #{ btn || '马上提交'}
