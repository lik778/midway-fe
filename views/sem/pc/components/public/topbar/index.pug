.topbar-bg
	.topbar
		a.left(href="https://www.baixing.com" target="_blank")
			img(src="//file.baixing.net/202105/bdb4590a977ec65849b73aea786e23eb.png", alt="logo")
			span 合作商家
		.right 
			if userInfo
				a#topbarPersenCenter.link-right(href="//www.baixing.com/w/posts?src=topbar", target="_blank", rel="nofollow") 个人中心
				a#topbarShopPromote.link-right(href="//www.baixing.com/a/pro/?src=topmenu", target="_blank", rel="nofollow") 商户推广
			else 
				a#topbarRegister.link-right(href="//www.baixing.com/oz/verify/reg", rel="nofollow", data-toggle="userAuth", data-type="reg") 注册
				a#topbarLogin.link-right(href="//www.baixing.com/oz/login", rel="nofollow", data-toggle="userAuth", data-type="login") 登录
			a#topbarHelp.link-right(href="//www.baixing.com/help/", target="_blank", rel="nofollow") 帮助
