      img.baixing-logo(src="//file.baixing.net/202105/3252caa19b257e704b3e63ce31c5216f.png", alt="logo")
    .head-search
      input#inputValue.input(onfocus="this.select();return false;" placeholder="" data-category="root" value="" x-webkit-speech="" name="query" autocomplete="off" required)
