//-在线客服
- const { totalTip } = renderData.basic.shop.color
.customer-service
	.customer-service__group
		if renderData.kf53
			.service-item#kf53(style=`background-color: ${totalTip || '#fff'}`)
				.service-logo.kf53
				.service-name 在线咨询
		.service-item#qq(style=`background-color: ${totalTip || '#fff'}`)
			.service-logo.qq(style=`background-image: url(${totalTip ? '//file.baixing.net/202205/a2075fd6a3ea9d4eee6e24e74eb97a31.png' : '//file.baixing.net/202102/18462324f0fffdb03b4f3a82da6b4be6.png'})`)
			.service-name(style=`color: ${totalTip ? '#fff' : '#333'}`) QQ交谈
			.service-modal
				if qq && qq.length>0
					each item in qq
						.service-modal__qq
							.qq-name #{item.name}
							.qq-link
								a(href="http://wpa.qq.com/msgrd?v=3&uin="+item.content+"&site=qq&menu=yes" target="_blank")
									img(src="//file.baixing.net/202102/bce82c406d3f290168828ffeb88d063a.png", alt="qq icon")
									span 联系客服
				else
					.service-modal__qq
						.qq-name 暂未添加QQ
		.service-item#mobile(style=`background-color: ${totalTip || '#fff'}`)
			.service-logo.mobile(style=`background-image: url(${totalTip ? '//file.baixing.net/202205/efe2c710de4579e8774b5c3030e922be.png' : '//file.baixing.net/202102/2a1a28b5fb72e227b0178c341341addd.png'})`)
			.service-name(style=`color: ${totalTip ? '#fff' : '#333'}`) 联系电话
			.service-modal
				.service-modal__mobile
					.mobile-title 咨询电话
					.mobile-number #{ phone.content }
					if phone2.content
						.mobile-number #{ phone2.content }
					.mobile-info-people 联系人：#{ contactName.content }
					.mobile-info-time 工作时间：7*24
		.service-item#wechat(style=`background-color: ${totalTip || '#fff'}`)
			.service-logo.wechat(style=`background-image: url(${totalTip ? '//file.baixing.net/202205/fb0d0aac79ee6033846cfe0de5c0533f.png' : '//file.baixing.net/202102/a278452bf26c6a6b4b870c69d2bcd615.png'})`)
			.service-name(style=`color: ${totalTip ? '#fff' : '#333'}`) 联系微信
			if weChat.content
				.service-modal
					.service-modal__wechat
						.mobile-title 微信号码
						.mobile-number #{ weChat.content }
						textarea#wechat-id-textarea #{ weChat.content }
						.click-copy 复制号码
			else
				.service-modal
					.service-modal__wechat
						.mobile-title 暂未添加微信号

	.customer-service__back-to-top
		#back-to-top
			.back-to-top__icon
			.back-to-top__text 返回顶部

	.customer-service__group
		.service-item#bax
				.service-logo.bax
				.service-name.bax 百姓网平台保障
				.service-modal 
					.service-modal__bax 
						.bax-title 投诉热线:
						.bax-number 021-61300238

