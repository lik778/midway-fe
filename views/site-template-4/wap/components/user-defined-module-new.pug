mixin user-defined-module-new(configItem)
	- const defaultImg = ['//file.baixing.net/202205/f5230b305ca647958a793dcbadf3a9a2.png','//file.baixing.net/202205/8c3cf87a774df6ceeff8661c7a4b37c0.png','//file.baixing.net/202205/4c877e95806508ea2c34895c5c1c6877.png','//file.baixing.net/202107/4c4c8531457a49f12b432243a20d6fce.png','//file.baixing.net/202107/ed8b66237a86e68c23f369854fe6fcc0.png','//file.baixing.net/202107/eabfa36d86e81cd4eb1031f3ace065a6.png','//file.baixing.net/202107/7665f5e7a4f21c9d1e864408b3ede10a.png','//file.baixing.net/202107/4a17dbc06030214723ec295899c63d65.png','//file.baixing.net/202107/eb3969456c6b4995d648a175a1a091a2.png','//file.baixing.net/202107/9250dd40e68112fbc54ebaef9c8006ff.png']
	- themeColor = renderData.basic.shop.color.productHv
	- specialIconColor = renderData.basic.shop.color.wap.specialIcon
	.block-container
		- const {subModuleBos, mainModuleTitle} = configItem
		.block-title #{mainModuleTitle}
			.prefix-box(style={background: specialIconColor || themeColor})
		.block-content
			each item,index in subModuleBos
				.user-defined-item
					img.img(src=item.urlImg ? item.urlImg : defaultImg[index])
					.content
						.title(style={color: specialIconColor || themeColor}) #{ item.title }
						.line(style={background: specialIconColor || themeColor})
						.desc #{ item.content }
