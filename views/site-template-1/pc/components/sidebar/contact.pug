//-- const companyInfo= renderData.basic.company
.contact
	dl.clearfix
		dt 公司名称：
		dd #{ companyInfo.name }
		dt 公司地址：
		dd #{ companyInfo.address }
		dt 营业电话：
		dd
			span #{ renderData.contact.phone.content }
		if renderData.contact.qq && renderData.contact.qq.length>0
			each item in renderData.contact.qq
				dt #{item.name}:
				dd
					a(href="http://wpa.qq.com/msgrd?v=3&uin="+item.content+"&site=qq&menu=yes" target="_blank")
						img(src="//file.baixing.net/202011/be5237a19752ef6d8ec6a4c2d054eaec.png", alt="")

