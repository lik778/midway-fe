<!-- Swiper -->
- const bannerList= renderData.banner
  .swiper-container
    .swiper-wrapper
      if bannerList && bannerList.length > 0
        each item in bannerList
          .swiper-slide(style="background-image:url(" + item.displayImgUrl + ")" )
      else
        .swiper-slide(style="background-image:url(//file.baixing.net/202011/ae7db439a3eb3331bfba1a69a9e069c8.png)")
        .swiper-slide(style="background-image:url(//file.baixing.net/202011/13cb77180ac3d0e492baf4a82b34275c.png)")

    <!-- Add Pagination -->
    .swiper-pagination
