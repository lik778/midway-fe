//-我先设置无数据情况下
- const bannerList= []
  .swiper-container
    .swiper-wrapper
      if bannerList && bannerList.length > 0
        each item in bannerList
          .swiper-slide
            img(src=item.displayImgUrl, alt="")
      else
        .swiper-slide
          img(src="//file.baixing.net/202011/ae7db439a3eb3331bfba1a69a9e069c8.png", alt="")
        .swiper-slide
          img(src="//file.baixing.net/202011/13cb77180ac3d0e492baf4a82b34275c.png", alt="")

    <!-- Add Pagination -->
    .swiper-pagination
