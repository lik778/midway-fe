extends ../layout.pug
block styles
    :combine(fileName="site-template-1-news-child-wap.css")

block header
    include ../components/header

block content
    - const cateList = renderData.articleCateList
    include ../components/toptab

    include ../components/news-list

    - const { totalPage } = renderData.articleList
    include ../components/pagination
    +pagination(totalPage, renderData.currentPage, `${currentPathname}`)


block scripts
    :combine(fileName="site-template-1-news-child-wap.js")
